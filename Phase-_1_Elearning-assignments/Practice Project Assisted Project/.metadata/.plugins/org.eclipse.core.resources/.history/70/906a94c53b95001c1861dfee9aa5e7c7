package com;

class MyException extends Exception{
	public MyException() {
		
	}
public MyException(String msg) {
	super(msg);
}
}
public class ThrowKeywordExample{
	
	public static void main(String[] args) {
		int a=10;
		int b=5;
		try {
			if(a>b) {
				throw new MyException("a>b");
			}
		}
		
		catch(Exception e) {
			System.out.println(e);
		}
}
}

